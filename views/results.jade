extend layout
block content
  p
    | Below are your results for your search: #{subject}
  p 
 
    
    |Here are the results:
    ul
      each obj, key in results
        each link in obj.articleLinks
          each title in obj.articleTitles
            li
              a(href=link, target='_blank') #{title} 
          
   
       
          
    
  block scripts  
   script(src="https://code.jquery.com/jquery-3.3.1.min.js", defer)
   script(src="js/index.js", defer)